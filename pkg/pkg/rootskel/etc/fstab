devpts 			/dev/pts	devpts	defaults,nosuid,noexec 0 0
tmpfs			/tmp		tmpfs	defaults,nosuid,nodev 0 0
tmpfs			/var/lock	tmpfs	defaults,nosuid,nodev,noexec 0 0
tmpfs			/var/run	tmpfs	defaults,nosuid,nodev,noexec 0 0
